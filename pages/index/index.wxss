.home-index .home-swiper swiper {
  height: 340rpx;
}
.home-index .home-swiper swiper image {
  width: 100%;
}
.home-index .home-navigat {
  display: flex;
}
.home-index .home-navigat navigator {
  flex: 1;
  padding: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
}
.home-index .home-navigat navigator image {
  width: 100%;
}
.home-index .home-floor .floor-group .floor-title {
  width: 100%;
  padding: 10rpx 10rpx;
  color: var(--themeColor);
  font-weight: bold;
}
.home-index .home-floor .floor-group .foolr-content {
  overflow: hidden;
}
.home-index .home-floor .floor-group .foolr-content navigator {
  float: left;
  width: 33.33%;
}
.home-index .home-floor .floor-group .foolr-content navigator:nth-child(-n + 5) {
  height: 27.72711207vw;
  border-left: 10rpx solid #ffffff;
}
.home-index .home-floor .floor-group .foolr-content navigator:nth-child(1) {
  height: 386rpx;
}
.home-index .home-floor .floor-group .foolr-content navigator:nth-child(2),
.home-index .home-floor .floor-group .foolr-content navigator:nth-child(3) {
  border-bottom: 10rpx solid #ffffff;
}
.home-index .home-floor .floor-group .foolr-content navigator:nth-child(3),
.home-index .home-floor .floor-group .foolr-content navigator:nth-child(5) {
  border-right: 10rpx solid #ffffff;
}
.home-index .home-floor .floor-group .foolr-content navigator image {
  width: 100%;
  height: 100%;
}
